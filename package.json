{
  "name": "ncku",
  "version": "1.0.0",
  "description": "成大課程作業",
  "app": "dist/index.html",
  "targets": {
    "app": {
      "publicUrl": "./"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:hw1-client": "esbuild ./hw1/client/app.tsx --jsx-factory=h --jsx-fragment=Fragment --bundle --sourcemap --outfile=./hw1/build/client/src/index.js",
    "build:hw1-server": "esbuild ./hw1/server/index.ts --bundle --outfile=./hw1/build/server.js --sourcemap --platform=node",
    "build:hw1-report": "esbuild ./hw1/report/dev/index.tsx --jsx-factory=h --jsx-fragment=Fragment --bundle --outfile=./hw1/report/build/index.js --sourcemap  --loader:.svg=dataurl",
    "build:hw2-data": "esbuild ./hw2/dl_data.ts --bundle --outfile=./hw2/dl_data.js --sourcemap  --platform=node",
    "build:hw2-client": "esbuild ./hw2/client/dev/app.tsx --jsx-factory=h --jsx-fragment=Fragment --bundle --sourcemap --outfile=./hw2/client/build/index.js",
    "build:hw3-client": "esbuild ./hw3/client/dev/app.tsx --jsx-factory=h --jsx-fragment=Fragment --bundle --sourcemap --outfile=./hw3/client/build/index.js",
    "build:hw4-client": "esbuild ./hw4/client/dev/app.tsx --jsx-factory=h --jsx-fragment=Fragment --bundle --sourcemap --outfile=./hw4/client/build/index.js --loader:.tsv=text"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toonnyy8/ncku.git"
  },
  "keywords": [],
