  },
  "homepage": "https://github.com/toonnyy8/ncku#readme",
  "dependencies": {
    "@antv/g2": "^4.1.32",
    "@tensorflow/tfjs": "^3.11.0",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/xml2js": "^0.4.9",
    "compromise": "^13.11.4",
    "compromise-sentences": "^0.3.0",
    "esbuild": "^0.13.3",
    "koa": "^2.13.3",
    "koa-router": "^10.1.1",
    "koa-session": "^6.2.0",
    "node-fetch": "^2.6.5",
    "nouislider": "^15.5.0",
    "porter-stemmer": "^0.9.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.1.0",
    "vue": "^3.2.19",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.13",
    "@types/koa-session": "^5.10.4",
    "@types/node": "^16.10.2",
    "@types/node-fetch": "^2.5.12"
  }
